{
    "env": {
        "browser": true,
        "react-native/react-native": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },

    "plugins": ["react", "react-native"],
    "rules": {
        "eslint-disable-next-line": "off",
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off"
    }
}
