{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "prettier", "plugin:import"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react", "react-native"],
    "rules": {
        "eslint-disable-next-line": "off",
        "react/react-in-jsx-scope": "off",
        "import/no-unresolved": "error",
        "react-native/no-unused-styles": "error"
    }
}
